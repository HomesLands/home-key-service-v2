<!-- views/table.ejs -->

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Table Template</title>
</head>
<body>

    <h1>My Table</h1>

    <table border="1">
        <thead>
            <tr>
                <th>Column 1</th>
                <th>Column 2</th>
                <th>Column 3</th>
            </tr>
        </thead>
        <tbody>
            <% for (let i = 0; i < data.length; i++) { %>
                <tr>
                    <td><%= data[i].column1 %></td>
                    <td><%= data[i].column2 %></td>
                    <td><%= data[i].column3 %></td>
                </tr>
            <% } %>
        </tbody>
    </table>

</body>
</html>
